		<dict>
			<key>Identifier</key>
			<string>accessModifier</string>
			<key>Required</key>
			<true/>
			<key>Name</key>
			<string>Access Modifier:</string>
			<key>Description</key>
			<string>The access modifier to use</string>
			<key>Type</key>
			<string>popup</string>
			<key>Default</key>
			<string>Internal</string>
			<key>Values</key>
			<array>
				<string>Internal</string>
				<string>Public</string>
				<string>Package</string>
			</array>
		</dict>
	</array>
